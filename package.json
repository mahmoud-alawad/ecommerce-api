{
  "name": "vue_node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "nodemon --config ./nodemon.config.json",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only && yarn prisma generate",
    "db:push": "npx prisma db push",
    "build": "rimraf public/server && tsc -p tsconfig.json",
    "type-check": "vue-tsc --noEmit",
    "prettier": "prettier --single-quote --trailing-comma all --write",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@prisma/client": "4.12.0",
    "@types/nodemailer": "^6.4.7",
    "autoprefixer": "^10.4.13",
    "body-parser": "^1.19.0",
    "class-validator": "^0.14.0",
    "cookie-session": "^2.0.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "redis": "^4.6.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/body-parser": "^1.19.0",
    "@types/chart.js": "^2.9.37",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.1.4",
    "@types/redis": "^4.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.12.0",
    "ts-node": "^10.8.1",
    "typescript": "~5.0.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.mjs"
  },
  "project_dir": "",
  "type": "commonjs"
}
